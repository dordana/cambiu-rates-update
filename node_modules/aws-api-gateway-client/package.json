{
  "name": "aws-api-gateway-client",
  "version": "0.2.7",
  "description": "A moduel for AWS API Gateway client",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kndt84/aws-api-gateway-client.git"
  },
  "keywords": [
    "aws",
    "apigateway"
