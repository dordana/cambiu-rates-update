  },
  "homepage": "https://github.com/kndt84/aws-api-gateway-client#readme",
  "main": "dist/apigClient.js",
  "scripts": {
    "transpile": "babel src -d dist/",
    "prepublish": "npm run lint && npm run transpile",
    "lint": "node_modules/eslint/bin/eslint.js src lib *.js",
    "test": "npm run lint && nyc --reporter=text ava",
    "test:watch": "ava --watch"
  },
  "files": [
    "dist",
    "dist-es"
  ],
  "ava": {
    "require": [
      "babel-register",
      "babel-polyfill"
    ],
    "babel": "inherit",
    "files": [
      "test/**/*.test.js"
    ]
  },
  "babel": {
    "presets": [
      "@ava/stage-4",
      "es2017",
      "es2016",
      "es2015",
      "stage-0"
    ],
    "plugins": [
      "transform-es5-property-mutators"
    ]
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": [
      "eslint:recommended",
      "google"
    ],
    "rules": {
      "max-len": [
        "error",
        100
      ],
      "require-jsdoc": "off",
      "no-undef": "off",
      "import/extensions": "off",
      "import/no-extraneous-dependencies": "off"
    }
  },
  "dependencies": {
    "axios": "^0.16.1",
    "axios-retry": "^1.2.0",
    "crypto-js": "^3.1.9-1",
    "url": "^0.11.0",
    "url-template": "^2.0.8"
  },
  "devDependencies": {
    "@ava/babel-preset-stage-4": "^1.0.0",
    "ava": "^0.18.2",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.1",
    "babel-plugin-transform-es5-property-mutators": "^6.24.1",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2016": "^6.24.1",
    "babel-preset-es2017": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.24.1",
    "eslint": "^3.19.0",
    "eslint-config-google": "^0.7.1",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "nyc": "^10.2.0"
  },
